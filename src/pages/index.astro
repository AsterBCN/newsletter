---
import Layout from '../layouts/Layout.astro';
import Head from "../components/Head.astro";
import Main1 from '../components/Main1.astro';
import Footer from '../components/Footer.astro';
import Styles from '../components/Styles.astro';
import { getNewsletters } from '../services/getnewsletters'

const {sequence, date, widthPhoto, news } = await getNewsletters()
---

<Layout>
  <Head seq={sequence} date={date}/>
  <Main1 news={news} widthPhotoDefault={widthPhoto}/>
  <Footer/>
  <Styles />
</Layout>